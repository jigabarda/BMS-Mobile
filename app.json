{
  "expo": {
    "name": "bms-mobile",
    "slug": "bms-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bmsmobile",
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "0461666f-dac5-4dac-ab61-80b99426f989"
      },
      "router": {}
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "NOTIFICATIONS",
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "package": "com.jagabarda.bmsmobile"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "jagabarda"
  }
}
